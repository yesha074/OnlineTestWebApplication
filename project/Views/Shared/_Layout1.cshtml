<!DOCTYPE html>
<html>
<head>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <meta name="viewport" content="width=device-width" />
    <title>Dashboard</title>
   
</head>
<body>
    <div class="row">
        <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2" style="height:1000px;border:1px solid gray; background-color:Highlight">
            <div style="width:110%;height:500px;" class="container list-group">
                <div class="list-group-item">
                    <img src="~/Content/images/2.jpg" style="height:100%;width:109%;" />
                </div>
                <div class="list-group-item">
                    @Html.ActionLink("Dashboard", "Dashboard", new { @class = "list-group-item" })
                </div>
            
                <div class="list-group-item">
                    @Html.ActionLink("Add Category", "Addcategory", new { @class = "list-group-item" })
                </div>

                <div class="list-group-item">
                    @Html.ActionLink("Add Questions", "Addquestions", new { @class = "list-group-item" })
                </div>

                <div class="list-group-item">
                    @Html.ActionLink("Logout", "Logout", new { @class = "list-group-item" })
                </div>


            </div>

        </div>
        <div class="col-lg-10 col-md-10 col-sm-10 col-xs-10" style="height:1000px;border:1px solid gray">

            @RenderBody()
        </div>
       
    </div>
  
</body>
</html>
